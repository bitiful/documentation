    {
      "source": "/BitifulS4/:slug*",
      "destination": "/bitiful-s4/:slug*"
    },{
      "source": "/bitiful-s4/what-is-bitiful-s4",
      "destination": "/bitiful-s4/intro"
    },{
      "source": "/intro",
      "destination": "/bitiful-s4/intro"
    },{
      "source": "/sdk",
      "destination": "/developer/sdk/intro"
    },{
      "source": "/sdk/:slug*",
      "destination": "/developer/sdk/:slug*"
    },{
      "source": "/category/:slug*",
      "destination": "/bitiful-s4/intro"
    }
  ],
