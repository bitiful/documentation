  },
  "favicon": "/favicon-docs.svg",
  "modeToggle": {
    "default": "dark"
  },
  "colors": {
    "primary": "#0D9373",
    "light": "#07C983",
    "dark": "#0D9373",
    "anchors": {
      "from": "#0D9373",
      "to": "#07C983"
    }
  },
  "backgroundImage":"https://bitiful-contents.s3.bitiful.net/images/bitiful-docs-blur-bg.svg",
  "topbarLinks": [
    {
      "name": "官网",
      "url": "https://www.bitiful.com"
    },
    {
      "name": "更新记录",
      "url": "/changelog/overview"
    }
  ],
  "topbarCtaButton": {
    "name": "控制台",
    "url": "https://console.bitiful.com"
  },
  "primaryTab": {
    "name": "产品文档"
  },
  "tabs": [
    {
      "name": "计费与价格",
      "url": "prices"
    },
    {
      "name": "API & SDK",
      "url": "developer"
    },
    {
      "name": "方案与实践",
      "url": "best-practice"
    },
    {
      "name": "更新记录",
      "url": "changelog"
    }
  ],
  "redirects": [
    {
      "source": "/docs/:slug*",
      "destination": "/:slug*"
