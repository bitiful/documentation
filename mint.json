{
  "$schema": "https://mintlify.com/schema.json",
  "name": "缤纷云文档",
  "logo": {
    "dark": "https://bitiful-contents.s3.bitiful.net/images/bitiful-Docs-Logo-V2-dark.svg",
    "light": "https://bitiful-contents.s3.bitiful.net/images/bitiful-Docs-Logo-V2-light.svg"
  },
  "favicon": "/favicon-docs.svg",
  "modeToggle": {
    "default": "dark"
  },
  "colors": {
    "primary": "#0D9373",
    "light": "#07C983",
    "dark": "#0D9373",
    "anchors": {
      "from": "#0D9373",
      "to": "#07C983"
    }
  },
  "backgroundImage":"https://bitiful-contents.s3.bitiful.net/images/bitiful-docs-blur-bg.svg",
  "topbarLinks": [
    {
      "name": "官网",
      "url": "https://www.bitiful.com"
    },
    {
      "name": "更新记录",
      "url": "https://www.bitiful.com/blog"
    }
  ],
  "topbarCtaButton": {
    "name": "控制台",
    "url": "https://console.bitiful.com"
  },
  "primaryTab": {
    "name": "产品文档"
  },
  "tabs": [
    {
      "name": "计费与价格",
      "url": "prices"
    },
    {
      "name": "API & SDK",
      "url": "sdk"
    },
    {
      "name": "应用与实践",
      "url": "best-practice"
    }
  ],
  "redirects": [
    {
      "source": "/docs/:slug*",
      "destination": "/:slug*"
    },
    {
      "source": "/BitifulS4/:slug*",
      "destination": "/bitiful-s4/:slug*"
    },{
      "source": "/bitiful-s4/what-is-bitiful-s4",
      "destination": "/bitiful-s4/intro"
    },{
      "source": "/intro",
      "destination": "/bitiful-s4/intro"
    },{
      "source": "/sdk",
      "destination": "/sdk/intro"
    },{
      "source": "/category/:slug*",
      "destination": "/bitiful-s4/intro"
    }
  ],
  "navigation": [
    {
      "group": "S4 对象存储",
      "pages": [
        "bitiful-s4/intro",
        "bitiful-s4/handbook/quick-start",
        {
          "group": "产品说明",
          "pages": [
            "bitiful-s4/use-case",
            "bitiful-s4/limits",
            "bitiful-s4/definitions"
          ]
        },
        {
          "group": "CoreIX 媒体处理",
          "pages": [
            "coreix/basic",
            "coreix/hdr",
            {
              "group": "处理参数",
              "pages": [
                "coreix/features/resize",
                "coreix/features/crop",
                "coreix/features/quality",
                "coreix/features/sharp",
                "coreix/features/blur",
                "coreix/features/watermark-image",
                "coreix/features/watermark"
              ]
            }
          ]
        }
      ]
    },
    {
      "group": "计费与价格",
      "pages": [
        "prices/basic",
        {
          "group": "计费细则",
          "pages": [
            "prices/detail/storage-prices",
            "prices/detail/transfer-prices",
            "prices/detail/requests-prices",
            "prices/detail/processing-prices"
          ]
        },
        "prices/compare",
        "prices/arrearage"
      ]
    },
    {
      "group": "内容分发 CDN",
      "pages": [
        "cdn/intro"
      ]
    },
    {
      "group": "Framer 加速",
      "pages": [
        "framerplus/intro"
      ]
    },
    {
      "group": "最佳实践",
      "pages": [
        "best-practice/web-uploads",
        "best-practice/auto-detect-mime",
        "best-practice/token-link"
      ]
    },
    {
      "group": "SDK 接入 S4",
      "pages": [
        "sdk/intro",
        "sdk/php-demo/start-to-use",
        "sdk/golang-demo/start-to-use",
        "sdk/java-demo/start-to-use",
        "sdk/python3-demo/start-to-use",
        "sdk/android-demo/start-to-use"
      ]
    }
  ],
  "footerSocials": {
    "website": "https://www.bitiful.com/"
  }
}
